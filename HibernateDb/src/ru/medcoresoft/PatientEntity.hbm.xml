<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.medcoresoft.PatientEntity" table="patient" schema="" catalog="pacs">
        <id name="id">
            <column name="Id" sql-type="varchar" length="16" not-null="true"/>
        </id>
        <property name="name">
            <column name="Name" sql-type="varchar" length="64"/>
        </property>
        <property name="birthDate">
            <column name="BirthDate" sql-type="date" length="10"/>
        </property>
    </class>
</hibernate-mapping>